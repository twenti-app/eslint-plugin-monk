{
  "name": "eslint-plugin-monk",
  "description": "eslint plugin to sort your imports and exports",
  "version": "0.0.2",
  "author": "arrastia@pm.me",
  "license": "MIT",
  "main": "build/index.js",
  "scripts": {
    "clean": "rimraf build/",
    "copy-files": "copyfiles -f \"src/plugin/**/*\" build/",
    "prepublishOnly": "npm run build",
    "build": "npm run clean && npm run copy-files"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twenti-app/eslint-plugin-monk.git"
  },
  "keywords": [
    "import",
    "export",
    "imports",
    "exports",
    "import-export",
    "monk"
  ],
  "bugs": {
    "url": "https://github.com/twenti-app/eslint-plugin-monk/issues"
  },
  "homepage": "https://github.com/twenti-app/eslint-plugin-monk#readme",
  "devDependencies": {
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@types/eslint": "^8.21.0",
    "@types/node": "^18.11.19",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@typescript-eslint/utils": "^5.51.0",
    "copyfiles": "^2.4.1",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.5"
  }
}
